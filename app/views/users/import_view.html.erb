<div class="container">
  <div class="row">
    <%= form_for :attachment, url: { controller: 'users', action: 'import_users' }, html: { multipart: true } do |f| %>
      <fieldset>
          <%= f.label :name %>
          <%= f.text_field :name %>
          <%= f.label :attachment %>
          <%= f.file_field :attachment %>
          <%= f.submit "Upload CSV", class: "btn btn-primary" %>
        </fieldset>
    <% end %>
  </div>
</div>
